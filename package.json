    "fs-extra": "^10.0.0",
    "mime": "^2.6.0",
    "open": "^7.0.3",
    "path": "^0.12.7",
    "read-last-lines": "^1.8.0",
    "serve-static": "^1.14.1",
    "url": "^0.11.3",
    "ws": "^8.17.0",
    "yazl": "^2.5.1"
